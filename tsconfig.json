{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
  },
  "include": [
    "src/**/*.ts" /* Include every ts file in source folder */
  ],
  "exclude": [
    "node_modules" /* exclude everything in  node_modules */
  ]
}